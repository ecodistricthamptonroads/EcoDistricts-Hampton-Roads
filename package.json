{
  "name": "eco-districts",
  "version": "1.0.0",
  "description": "Eco Districts Hampton Roads",
  "private": true,
  "main": "server.js",
  "author": "EcoBits",
  "license": "MIT",
  "scripts": {
    "precommit": "lint-staged",
    "build": "webpack --mode production",
    "dev": "concurrently -k \"npm run frontend\" \"npm run backend\"",
    "frontend": "webpack-dev-server --mode development",
    "backend": "nodemon server.js",
    "test": "mocha --compilers js:babel-core/register ./test/**/*.js --require ignore-styles"
  },
  "babel": {
    "presets": [
      "env",
      "react"
    ],
    "plugins": [
      "transform-object-rest-spread",
      "react-hot-loader/babel",
      [
        "transform-runtime",
        {
          "polyfill": false,
          "regenerator": true
        }
      ]
    ]
  },
  "nodemonConfig": {
    "ignore": [
      "frontend/**",
      "public/**"
    ]
  },
  "lint-staged": {
    "**/*.{js,jsx,json,css}": [
      "prettier --single-quote --write",
      "git add"
    ]
  },
  "dependencies": {
    "@toast-ui/react-editor": "^2.5.0",
    "aws-sdk": "^2.773.0",
    "axios": "^0.18.0",
    "body-parser": "^1.18.2",
    "bootstrap": "^4.5.3",
    "bootstrap-switch-button-react": "^1.2.0",
    "connect-busboy": "0.0.2",
    "cookie-session": "^2.0.0-beta.3",
    "csv": "^2.0.0",
    "express": "^4.15.2",
    "express-busboy": "^7.0.1",
    "express-fileupload": "^1.1.6",
    "mongoose": "^5.10.9",
    "multer": "^1.4.2",
    "node-sass": "^4.9.0",
    "nodemailer": "^6.4.14",
    "passport": "^0.4.0",
    "passport-google-oauth20": "^1.0.0",
    "prop-types": "^15.5.8",
    "qs": "^6.5.1",
    "react": "^16.14.0",
    "react-bootstrap": "^1.0.0-beta.12",
    "react-dom": "^16.14.0",
    "react-hot-loader": "^4.13.0",
    "react-mailchimp-subscribe": "^2.1.0",
    "react-markdown": "^5.0.2",
    "react-redux": "^7.1.1",
    "react-router": "^4.2.0",
    "react-router-dom": "^5.0.1",
    "react-search-box": "^2.0.1",
    "react-select": "^1.2.1",
    "react-slideshow-image": "^3.4.2",
    "react-transition-group": "^2.2.1",
    "redux": "^4.0.4",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "concurrently": "^3.5.0",
    "css-loader": "^0.28.0",
    "enzyme": "^3.3.0",
    "eslint": "^4.19.1",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.21.4",
    "file-loader": "^1.1.11",
    "husky": "^0.14.3",
    "ignore-styles": "^5.0.1",
    "lint-staged": "^7.0.2",
    "mocha": "^5.0.5",
    "nodemon": "^1.17.5",
    "prettier": "^1.11.1",
    "react-addons-test-utils": "^15.5.1",
    "react-test-renderer": "^16.14.0",
    "redux-devtools": "^3.4.0",
    "redux-devtools-dock-monitor": "^1.1.2",
    "redux-devtools-log-monitor": "^1.3.0",
    "sass-loader": "^6.0.3",
    "style-loader": "^0.20.3",
    "url-loader": "^1.0.1",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.8",
    "webpack-dev-middleware": "^3.1.0",
    "webpack-dev-server": "^3.8.0",
    "webpack-hot-middleware": "^2.18.0",
    "webpack-merge": "^4.1.2"
  },
  "css.lint.unknownAtRules": "ignore"
}
